{
  "name": "@unifydev/flexilla",
  "version": "0.1.2",
  "description": "Variants, shortcuts, utilities for flexilla",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "rimraf dist && unbuild",
    "format": "biome format --write ./src"
  },
  "files": [
    "dist",
    "!dist/index.d.cts",
    "!dist/index.d.mts",
    "README.md",
    "favicon-dark.png",
    "package.json",
    "!.gitignore",
    "!tsconfig.json",
    "!biome.js"
  ],
  "author": "Johnkat MJ",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@unocss/reset": "^0.58.6",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "unocss": "^v66.0.0"
  },
  "dependencies": {
    "@unifydev/unify-variant": "latest"
  },
  "keywords": [
    "uno preset",
    "unocss",
    "unocss component",
    "unocss ui",
    "unocss",
    "flexilla"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/unoforge/unify-preset"
  },
  "bugs": {
    "url": "https://github.com/unoforge/unify-preset/issues"
  }
}
