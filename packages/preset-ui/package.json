{
  "name": "@unifydev/preset-ui",
  "version": "1.2.5",
  "description": "UnoCSS Theming Toolkit, create modern and beautifull Web Ui",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./css/default/base.css": "./dist/css/default/base.css",
    "./css/default/dark.css": "./dist/css/default/dark.css",
    "./css/default/light.css": "./dist/css/default/light.css",
    "./themes/air.css": "./dist/css/themes/air.css",
    "./themes/water.css": "./dist/css/themes/water.css",
    "./themes/earth.css": "./dist/css/themes/earth.css",
    "./themes/fire.css": "./dist/css/themes/fire.css"
  },
  "scripts": {
    "format": "biome format --write ./src",
    "build": "rimraf dist && unbuild"
  },
  "files": [
    "dist",
    "README.md",
    "favicon-dark.png",
    "package.json",
    "!.gitignore",
    "!tsconfig.json",
    "!biome.js"
  ],
  "author": "Johnkat MJ",
  "license": "MIT",
  "dependencies": {
    "@unifydev/preset-bg": "latest",
    "@unifydev/unify-variant": "latest"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "unocss": "^v66.0.0"
  },
  "keywords": [
    "Preset UI",
    "unify preset",
    "atomic ui library",
    "unocss ui",
    "theming ui library",
    "unocss components",
    "modern ui",
    "component library",
    "unify ui",
    "unifyui",
    "uno ui",
    "unoui",
    "unoforge"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/unoforge/unify-preset"
  },
  "bugs": {
    "url": "https://github.com/unoforge/unify-preset/issues"
  },
  "gitHead": "1a4a3121332b2961338fd7920369d64ba3bab0e5"
}
